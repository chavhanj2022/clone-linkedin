<%= form_with model: current_user, url: update_profile_path, method: :patch do |f|%>
  <div class="modal-body">
    <div class="row mb-3">
      <div class="col-lg-12">
        <div class="form-group row">
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">First Name</label>
            <%= f.text_field :last_name, class: "form-control"%>
          </div>
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">Last Name</label>
            <%= f.text_field :last_name, class: "form-control"%>
          </div>
        </div >
      </div>
      <div class="col-lg-12">
        <div class="form-group row">
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">City</label>
            <%= f.text_field :city, class: "form-control"%>
          </div>
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">State</label>
            <%= f.text_field :state, class: "form-control"%>
          </div>
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">Country</label>
            <%= f.text_field :country, class: "form-control"%>
          </div>
          <div class="col-lg-6">
            <label class="form-label mb-3 mt-3">Pin-code</label>
            <%= f.text_field :pincode, class: "form-control"%>
          </div>
        </div >
      </div>
      <div class="col-lg-12">
        <label class="form-label mb-3 mt-3">Profile Title</label>
        <%= f.text_field :profile_title, class: "form-control"%>
      </div>
    </div>
    <div class="modal-footer">
      <%= f.submit "Save Changes", data: {action: "click->bs-modal#submitEnd"}, class: 'btn btn-primary'%>
      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
    </div>
  </div>
<% end %>
